
<view class="weui-cells__title">暂不支持显示图表</view>
<block wx:for="{{data}}" wx:for-item="items">
    <view class="weui-cells__title">{{items.name}}</view>
    <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_access" hover-class="weui-cell_active" wx:for="{{items.data}}" wx:for-item="item">
            <view class="weui-cell__bd">{{item.name}}</view>
            <view class="weui-cell__ft">￥{{item.y}}</view>
        </view>
    </view>
</block>